:80, :{$PORT} {
    root * /app/{$PUBLIC_DIR}
    php_fastcgi 127.0.0.1:9000
    file_server

    header / {
        X-XSS-Protection "1; mode=block"
        X-Content-Type-Options "nosniff"
        X-Frame-Options "ALLOW"
    }

    log {
        output stdout
        format console
    }

    push
    encode gzip
}
